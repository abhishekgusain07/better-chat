{
  "name": "chatapp-backend",
  "version": "1.0.0",
  "description": "Chat application backend with WebSocket support",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "express": "^4.19.2",
    "socket.io": "^4.8.1",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^17.2.1",
    "zod": "^3.25.0",
    "drizzle-orm": "^0.44.4",
    "@neondatabase/serverless": "^1.0.1",
    "better-auth": "^1.3.4",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^11.1.0",
    "nanoid": "^5.1.5",
    "@anthropic-ai/sdk": "^0.32.1",
    "openai": "^4.80.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.5",
    "ws": "^8.18.0",
    "@polar-sh/sdk": "^0.34.9",
    "express-rate-limit": "^7.5.0"
  },
  "devDependencies": {
    "@types/node": "^20.17.11",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^1.4.12",
    "@types/ws": "^8.5.14",
    "@types/jest": "^30.0.0",
    "typescript": "^5.7.3",
    "tsx": "^4.19.4",
    "jest": "^30.0.5",
    "ts-jest": "^29.4.1",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "drizzle-kit": "^0.31.4"
  },
  "keywords": [
    "chat",
    "ai",
    "websocket",
    "express",
    "typescript"
